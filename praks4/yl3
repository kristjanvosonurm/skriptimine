#!/bin/bash
#
#skripti algus
#
echo -n "Mitu reisijat on teie grupis?:"
read inimesed  K체sitleb inimeste arvu

echo -n "Mitu kohta on 체hes bussis?:"
read kohad #K체sib kohtasid

bussid=`expr $inimesed / $kohad`
ylejaanud=`expr $inimesed % $kohad`

echo "Kokku on vaja $bussid bussi ja kui see number $ylejaanud ei ole 0 ringis siis on vaja busse juurde."

# skripti l천pp






